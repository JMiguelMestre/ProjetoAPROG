#include <stdio.h>
#include <stdlib.h>
#define MAX 50

/* Estrutura Deputado */
typedef struct D{
char codigo[MAX];
char nome [MAX];
char genero;
char circulo[MAX];
}tDeputado;

/* Estrutura Deputados */
typedef tDeputado tDeputados[MAX];

/* Estrutura Partido */
typedef struct P{
char sigla[MAX];
char nome[MAX];
int qtd;
tDeputados deputados;
}tPartido;

/* Estrutura Partidos */
typedef tPartido tPartidos[MAX];

/* Estrutura Parlamento */
typedef struct Pa{
char pais;
tPartidos partidos;
}tParlamento;

void lerparlamentos(tParlamento vetor, int n){
    int i;
    for (i=0; i<n; i++){
        printf("%c :",vetor[0]);
        printf("%c - %c - %d deputados;", vetor[1][i][0],vetor[1][i][1],vetor[1][i][2] );
    }
}

void lerpartidos(tParlamento vetor, int p){
    int i,j;
    for (i=0;i<p;i++){
        printf("%c :", vetor[1][i][1]);
        for (j=0;j<vetor[1][i][2];j++){
            printf("%d - %c - %c - %c \n",vetor[1][3][j][0],vetor[1][3][j][1],vetor[1][3][j][2],vetor[1][3][j][3]);
        }
    }
}

